\set QUIET 1
\pset null '[NULL]'
\set PROMPT1 '%[%033[1;31m%]┌─(%[%033[0m%]%n%[%033[0m%]%[%033[1;31m%]@%[%033[1;0m%]%M:%>%[%033[1;33m%]/%/%[%033[1;31m%]%x%[%033[K%]%[%033[0m%]%[%033[1;31m%])\n└─%[%033[0m%]✜ '
\set PROMPT2 '%[%033[1;33m%]%R%#%[%033[0m%] '
\set paste
\pset pager always
\timing
\x auto
\set VERBOSITY verbose
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
\set FETCH_COUNT 1000
\unset QUIET
